import java.util.*;
import java.lang.*;
import java.io.*;
class GUVI
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int arr1[]=new int[2];
        int arr2[]=new int[2];
      arr1[0]=3;
      arr1[1]=4;
      arr2[0]=3;
      arr2[1]=4;
      int result=0;
      int sum=0;
      for(int i:arr1)
      {
      for(int j:arr2)
        sum=arr1[i]+arr2[j];
        result=result+sum;
	}
      System.out.println("sum is"+result);
    }
  
      
    }
